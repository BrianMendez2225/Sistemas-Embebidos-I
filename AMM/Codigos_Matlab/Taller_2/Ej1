clc; clear; close all;

% Definir las funciones de transferencia dadas
G1 = tf([1], [5 10]);
G2 = 100;
G3 = tf([2], [1 -1]);
G4 = 5;
H1 = 0.2;
H2 = 50;

% Reducci贸n paso a paso
G5 = series(G1, G4); % G1 * G4
G6 = feedback(G5, H1); % Retroalimentaci贸n con H1
G7 = parallel(G3, G2);
G8 = series(G6, G7);
Gtotal = feedback(G8, H2); % Retroalimentaci贸n con H2

% Mostrar resultado final
disp('La funci贸n de transferencia equivalente del sistema es:');
Gtotal
